      "easeInOutQuad":{
         "text":"easeInOutQuad",
         "description":"acceleration until halfway, then deceleration",
         "formula":"t<.5 ? 2t^2 : -1+(4-2t)t",
         "equation":"t<.5 ? 2*t*t : -1+(4-2*t)*t"
      },
      "easeInQuint":{
         "text":"easeInQuint",
         "description":"accelerating from zero velocity",
         "formula":"t^5",
         "equation":"t*t*t*t*t"
      },
      "easeOutQuint":{
         "text":"easeOutQuint",
         "description":"decelerating to zero velocity",
